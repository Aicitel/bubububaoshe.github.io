<?xml version='1.0' encoding='utf-8'?>
<widget id="gj3.fanworks.bubububaoshe.qqx" version="1.0.0" versionCode="10" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>千秋戏</name>
    <description>
   源自古剑奇谭3的卡牌插刀小游戏。
  </description>
    <author email="support@phonegap.com" href="https://weibo.com/3802484189/">
      bubububaoshe
  </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="landscape" />
    <preference name="fullscreen" value="true" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="~2.4.0">
        <variable name="XWALK_VERSION" value="23+" />
        <variable name="XWALK_LITEVERSION" value="xwalk_core_library_canary:17+" />
        <variable name="XWALK_COMMANDLINE" value="--disable-pull-to-refresh-effect" />
        <variable name="XWALK_MODE" value="embedded" />
        <variable name="XWALK_MULTIPLEAPK" value="true" />
    </plugin>
    <engine name="android" spec="^8.0.0" />
</widget>
